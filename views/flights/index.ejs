<%- include('../partials/header') %>
  <ul>
    <h1>All Flights</h1>
  </ul>

  <table id="list">
    <thead>
      <tr>
        <th>Airline</th>
        <th>Airport</th>
        <th>Flight No.</th>
        <th>Depart</th>
      </tr>
    </thead>
    <tbody>
      <% flights.forEach(function(f) { %>
        <tr>
          <td><%= f.airline %></td>
          <td><%= f.airport %></td>
          <td><%= f.flightNo %></td>
          <td><%= f.depart %></td>
        </tr>
        <% }); %>
      </tbody>
    </table>
<%- include('../partials/footer') %>